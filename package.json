{
  "name": "graphql-front-boilerplate",
  "version": "0.1.0",
  "private": true,
  "description": "This repository contains all of the work for graphql boilerplate",
  "main": "index.jsx",
  "scripts": {
    "start": "ENVIRONMENT=development webpack-dev-server -d --progress",
    "local": "SET ENVIRONMENT=development&& webpack-dev-server -d --progress",
    "build": "ENVIRONMENT=production webpack -p --progress && cp -a ./public/. ./build/",
    "deploy": "npm run build && aws s3 sync ./build/ s3://graphql-front-boilerplate.com --cache-control max-age=604800 --acl public-read --profile graphql-front-boilerplate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ]
  },
  "author": "Wozzo",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wozzocomp/graphql-front-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/wozzocomp/graphql-front-boilerplate/issues"
  },
  "homepage": "https://github.com/wozzocomp/graphql-front-boilerplate#readme",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@wozzocomp/base-comps": "^1.7.3",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link": "1.2.14",
    "apollo-link-error": "1.1.13",
    "apollo-link-http": "1.5.17",
    "apollo-upload-client": "^14.1.0",
    "apollo-utilities": "1.3.4",
    "classnames": "2.2.6",
    "dotenv": "8.2.0",
    "graphql": "15.3.0",
    "graphql-tag": "^2.10.4",
    "moment": "2.27.0",
    "prop-types": "15.7.2",
    "react": "^16.13.1",
    "react-apollo": "3.1.5",
    "react-dom": "^16.13.1",
    "react-quill": "1.3.5",
    "react-redux": "^7.2.1",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-airbnb": "5.0.0",
    "babel-preset-minify": "0.5.1",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "^4.0.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "^7.20.4",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.14.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "sass-loader": "^9.0.2",
    "speed-measure-webpack-plugin": "1.3.3",
    "style-loader": "1.2.1",
    "terser-webpack-plugin": "^3.0.8",
    "webpack": "^4.44.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
