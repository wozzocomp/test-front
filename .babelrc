{
  "env": {
      "production": {
          "presets": [
              "@babel/preset-env",
              "@babel/preset-react",
              "minifi"
          ],
          "plugins": [
              "transform-class-properties",
              "@babel/plugin-proposal-optional-chaining"
          ]
      },
      "development": {
          "presets": [
              "@babel/preset-env",
              "@babel/preset-react"
          ],
          "plugins": [
              "transform-class-properties",
              "@babel/plugin-proposal-optional-chaining"
          ]
      }
  }
}